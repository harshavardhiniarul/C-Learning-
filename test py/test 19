num1 = int(input())
num2 = int(input())
choice = int(input())

if choice == 1:
    result = num1 + num2
    print("Result =", result)
elif choice == 2:
    result = num1 - num2
    print("Result =", result)
elif choice == 3:
    result = num1 * num2
    print("Result =", result)
elif choice == 4:
    if num2 != 0:
        result = num1 // num2  # Use integer division // to match C behavior
        print("Result =", result)
    else:
        print("Division by zero is not allowed")
else:
    print("Invalid choice")
