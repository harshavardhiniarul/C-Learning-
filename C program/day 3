#include <stdio.h>
//To print the first five odd numbers
int main() {
    int count = 0;
    int number = 1;
    
    printf("The first 5 odd numbers are: ");


    while (count < 5) {
        printf("%d ", number);
        number += 2;   
        count++;        
    }

    printf("\n"); 
    
    return 0;
}
